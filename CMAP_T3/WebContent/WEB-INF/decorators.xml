<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF">

	<decorator name="main" page="common/main.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="login" page="views/login.jsp">
		<pattern>/login</pattern>
	</decorator>
	<decorator name="loginOIDC_MLC" page="views/login_openid_mlc.jsp">
		<pattern>/loginOIDC</pattern>
	</decorator>
	<decorator name="loginOIDC_NTPC" page="views/login_openid_ntpc.jsp">
		<pattern>/loginOIDC_NTPC</pattern>
	</decorator>
	<decorator name="formredirection" page="views/formredirection.jsp">
		<pattern>/login/authByOIDC_NTPC</pattern>
	</decorator>
	<decorator name="accessDenied" page="views/access_denied.jsp">
		<pattern>/accessDenied*</pattern>
	</decorator>
	<decorator name="test" page="views/test/test.jsp">
		<pattern>/___test___</pattern>
	</decorator>
	<decorator name="outsite" page="common/blank.jsp">
		<pattern>/outsite*</pattern>
	</decorator>
	<decorator name="prtg_welcome_iframe" page="views/prtg/welcome.jsp">
		<pattern>/prtg/welcome*</pattern>
	</decorator>
	<decorator name="vm_switch" page="views/plugin/module_vm_switch.jsp">
		<pattern>/plugin/module/vmswitch*</pattern>
	</decorator>
	<decorator name="version_diff_view" page="views/version/version_diff_view.jsp">
		<pattern>/version/diff/view*</pattern>
	</decorator>
</decorators>